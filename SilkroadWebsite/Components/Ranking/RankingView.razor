@page "/ranking"
@using SilkroadWebsite.Components.Ranking.Pages

@rendermode InteractiveServer
@inject NavigationManager Navigation

<PageTitle>Ranking</PageTitle>

<RadzenBreadCrumb>
    <RadzenBreadCrumbItem Path="/" Text="Home" />
    <RadzenBreadCrumbItem Text="Ranking" />
</RadzenBreadCrumb><br/>


<RadzenButton ButtonStyle="ButtonStyle.Warning" Variant="Variant.Flat" Shade="Shade.Darker" Icon="account_box" class="m-1" Click=@(() => Navigation.NavigateTo("/ranking/player")) Text="Player" />
<RadzenButton ButtonStyle="ButtonStyle.Danger" Variant="Variant.Flat" Shade="Shade.Darker" Icon="dashboard" class="m-1" Click=@(() => Navigation.NavigateTo("/ranking/guild")) Text="Guild" />
<br/> <br/>

@code {
    protected override async Task OnInitializedAsync()
    { }
}
